{
  "manifest_version": 3,
  "name": "iEMPT考題圖片合併PDF助手",
  "version": "3.0",
  "description": "自動下載網頁上的考題圖片並合併成一份 PDF",
  "permissions": [
    "activeTab",
    "scripting",
    "downloads"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}